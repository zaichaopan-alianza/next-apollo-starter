{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gql:generate": "yarn graphql-codegen",
    "migrate:dev": "yarn prisma migrate dev",
    "prisma:generate": "yarn prisma generate",
    "prisma:seed": "yarn prisma db seed",
    "predev": "concurrently \"yarn prisma:generate\" \"yarn migrate:dev\""
  },
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed/index.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@apollo/server": "^4.3.0",
    "@as-integrations/next": "^1.2.0",
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@next/font": "13.0.7",
    "@prisma/client": "^4.8.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.0",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.7",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^8.3.14",
    "loadsh": "^0.0.4",
    "next": "13.0.7",
    "next-auth": "^4.18.7",
    "node-fetch-native": "^1.0.1",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sqlite3": "^5.1.4",
    "ts-node": "^10.9.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/client-preset": "^1.2.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.11",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4"
  }
}
